{
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.2",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "undo-migrate": "sequelize-cli db:migrate:undo:all",
    "build": "tsc && npm run undo-migrate && npm run migrate && npm run seed",
    "heroku-cleanup": "rm -rf ./src",
    "seed": "tsc && sequelize-cli --options-path './.sequelizerc.dev' db:seed:all",
    "undo-seed": "sequelize-cli --options-path './.sequelizerc.dev' db:seed:undo:all",
    "start": "node dist/server.js",
    "dev": "tsc && node -r dotenv/config dist/server.js",
    "server": "nodemon ./src/server.js",
    "redo": "tsc && npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && sequelize-cli --options-path './.sequelizerc.dev' db:seed:all && node -r dotenv/config dist/server.js",
    "heroku-push": "git push -f heroku heroku:main"
  }
}
