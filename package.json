{
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^3.0.0",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.2",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/swagger-ui-express": "^4.1.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "undo-migrate": "sequelize-cli db:migrate:undo:all",
    "build": "tsc && npm run undo-migrate && npm run undo-seed && npm run migrate && npm run seed",
    "heroku-cleanup": "rm -rf ./src",
    "seed": "sequelize-cli db:seed:all",
    "undo-seed": "sequelize-cli db:seed:undo:all",
    "start": "node dist/server.js",
    "dev": "tsc && node -r dotenv/config dist/server.js"
  },
  "engines": {
    "node": "^16.15.1",
    "npm": "^8.11.0"
  }
}